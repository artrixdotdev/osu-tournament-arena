{
   "name": "@ota/svelte-config",
   "private": true,
   "type": "module",
   "exports": {
      ".": "./svelte.config.ts"
   },
   "license": "MIT",
   "scripts": {
      "build": "tsc",
      "clean": "git clean -xdf .cache .turbo dist node_modules",
