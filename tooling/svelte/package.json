   "devDependencies": {
      "@ota/prettier-config": "workspace:*",
      "@ota/tsconfig": "workspace:*",
      "eslint": "catalog:",
      "prettier": "catalog:",
      "typescript": "catalog:",
      "svelte": "catalog:",
      "@sveltejs/adapter-cloudflare": "7.2"
   },
   "prettier": "@ota/prettier-config"
}
